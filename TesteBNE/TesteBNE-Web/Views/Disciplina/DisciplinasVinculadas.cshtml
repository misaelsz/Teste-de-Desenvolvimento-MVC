
@{
	ViewBag.Title = "DisciplinasVinculadas";
	Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<div class="container">
	<h2>Disciplinas matriculadas</h2>
	<br /><br />
	<button type='button' onclick='getDisciplinasPorAluno()' class='btn btn-primary'>Listar</button>
	<br /><br />
	<table id="listaVinculadas" class="table">
		<thead class="thead-dark">
			<tr>
				<th scope="col">Nome</th>
			</tr>
		</thead>
		<tbody></tbody>
	</table>
	<br /><br /><br /><br /><br /><br />
	@Html.ActionLink("Lista de alunos", "IndexAluno", "Aluno", null, new { @class = "btn btn-success" })
</div>

@section scriptListarDisciplinas{
	<script src="~/Scripts/listarDisciplinas.js"></script>
	<script src="~/Servicos/ajax.js"></script>
}